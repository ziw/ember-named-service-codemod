{
  "name": "ember-named-service-codemod",
  "version": "0.1.0",
  "description": "A codemod to convert Ember.js service injections to include the service name argument ",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ziw/ember-named-service-codemod.git"
  },
  "bugs": {
    "url": "https://github.com/ziw/ember-named-service-codemod/issues"
  },
  "author": "Zi Wang",
  "homepage": "https://github.com/ziw/ember-named-service-codemod#readme",
  "scripts": {
    "lint": "eslint --cache .",
    "test": "codemod-cli test",
    "test:coverage": "codemod-cli test --coverage",
    "update-docs": "codemod-cli update-docs",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "bin": "./bin/cli.js",
  "keywords": [
    "codemod-cli"
  ],
  "dependencies": {
    "codemod-cli": "^3.1.2"
  },
  "devDependencies": {
    "coveralls": "^3.1.0",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1"
  },
  "engines": {
    "node": "10.* || 12.* || >= 14"
  },
  "jest": {
    "testEnvironment": "node"
  }
}
